# Backend environment template (no secrets)
# Copy to backend/.env and fill values locally. Do not commit secrets.

# Astra (secure bundle + token)
ASTRA_BUNDLE_PATH=secure-connect.zip
ASTRA_TOKEN=
ASTRA_KEYSPACE=default_keyspace

# Optional: alternative connection if needed
ASTRA_HOST=
ASTRA_PORT=
ASTRA_CLIENT_ID=
ASTRA_CLIENT_SECRET=

# App settings
ASSETS=btc-bitcoin,eth-ethereum
DAILY_START=2025-01-01

# Optional token if required by local scripts (leave empty)
GH_TOKEN=

